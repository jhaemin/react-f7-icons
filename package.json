{
  "name": "react-f7-icons",
  "version": "2.0.0-beta.1",
  "description": "A React SVG components library of Framework7 Icons",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "clone-f7-icons": "rimraf f7-icons && git clone https://github.com/framework7io/framework7-icons.git f7-icons",
    "generate": "node src/scripts/generate.mjs",
    "build": "npm run generate && rimraf dist && tsc && cp package.json dist/package.json && cp README.md dist/README.md",
    "deploy-website": "gh-pages -d test/out --dotfiles"
  },
  "author": "Jang Haemin",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.18",
    "change-case": "^4.1.2",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.2.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  }
}
