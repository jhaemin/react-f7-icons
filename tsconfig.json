{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "noEmit": false,
    "declaration": true,
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src/lib"]
}
